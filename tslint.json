{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "trailing-comma": [false],
    "no-console": false,
    "quotemark": [false],
    "arrow-parens": false,
    "ordered-imports": false,
    "object-literal-key-quotes": false,
    "max-line-length": [false],
    "object-literal-sort-keys": false,
    "semicolon": false
  },
  "rulesDirectory": []
}
